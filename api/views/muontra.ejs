<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Quản lý lịch sử mượn trả xe</title>

    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
    />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/usm/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css"
    />
    <link
      href="https://fonts.googleapis.com/css?family=Open+Sans"
      rel="stylesheet"
    />
    <!-- My css -->
    <link rel="stylesheet" href="css/login.css" />
    <!-- My js -->
    <script src="js/function.js" type="text/javascript"></script>

    <!-- Data table -->
    <link
      rel="stylesheet"
      type="text/css"
      href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="https://cdn.datatables.net/1.10.19/css/dataTables.bootstrap4.min.css"
    />

    <script src="https://code.jquery.com/jquery-3.3.1.js"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/dataTables.bootstrap4.min.js"></script>
  </head>

  <body>
    <script>
      $(document).ready(function() {
        $("#tbMuonTra").DataTable();
      });
    </script>

    <script></script>
    <div id="header">
      <%- include('header'); -%>
    </div>

    <div class="container-fluid bg-white ">
      <h2 class="text-center font-weight-semi-bold text-dark mt-3">
        QUẢN LÝ LỊCH SỬ MƯỢN TRẢ XE
      </h2>
      <div class="form-group form-inline col-lg-2">
        <label class="mr-3">Lọc</label>
        <select class="form-control form-control-sm" id="slMuonTra_View">
          <option value="1">Xem tất cả</option>
          <option value="0">Chưa trả xe</option>
        </select>
      </div>
      <div class="container-fluid">
        <!-- Table muon tra -->
        <div class="table-responsive">
          <table
            id="tbMuonTra"
            class="table table-striped table-bordered"
            style="width:100%"
            cellspacing="0"
            cellpadding="0"
          >
            <thead class="bg-info text-white font-weight-semi-bold text-center">
              <tr>
                <td>STT</td>
                <td>ID người dùng</td>
                <td>ID xe</td>
                <td>Thời gian mượn</td>
                <td>Thời gian trả</td>
                <td>Xem chi tiết</td>
              </tr>
            </thead>

            <tbody class="text-center">
              <% muontra.forEach(function(mt){ %>
              <tr>
                <td></td>
                <td><%= mt.TK_ID %></td>
                <td><%= mt.XE_ID %></td>
                <td><%= mt.MUON_THOIGIAN %></td>
                <td><%= mt.TRA_THOIGIAN %></td>
                <td>
                  <a href="#"
                    ><i
                      class="fa fa-info-circle fa-lg"
                      data-toggle="modal"
                      data-target="#ChiTietMuonTra"
                    ></i
                  ></a>
                </td>
              </tr>

              <% }); %>
            </tbody>
          </table>
        </div>

        <!-- Modal Chi tiet muon tra -->
        <div id="ChiTietMuonTra" class="modal fade" role="dialog">
          <div class="modal-dialog modal-lg">
            <!-- Modal content-->
            <div class="modal-content">
              <div class="modal-header">
                <h4 class="modal-title text-uppercase">Chi tiết mượn trả xe</h4>
                <button type="button" class="close" data-dismiss="modal">
                  &times;
                </button>
              </div>

              <div class="modal-body">
                <table class="table table-sm table-borderless col-lg-8 mx-auto">
                  <tr>
                    <td>ID mượn trả</td>
                    <td>1</td>
                  </tr>
                  <tr>
                    <td>ID người dùng</td>
                    <td>B1606935</td>
                  </tr>
                  <tr>
                    <td>ID xe</td>
                    <td>X001</td>
                  </tr>
                  <tr>
                    <td>Thời gian mượn</td>
                    <td>15:00:00 06/05/2019</td>
                  </tr>
                  <tr>
                    <td>Thời gian trả</td>
                    <td>15:15:00 06/05/2019</td>
                  </tr>
                  <tr>
                    <td>Vị trí mượn</td>
                    <td>
                      <img src="img/marker-green.png" height="20px" />
                      10.02998, 105.769601
                    </td>
                  </tr>
                  <tr>
                    <td>Vị trí trả</td>
                    <td>
                      <img src="img/marker-red.png" height="20px" />
                      10.03054, 105.768646
                    </td>
                  </tr>
                </table>
              </div>

              <!-- Map -->
              <div id="googleMap" style="width: 100%; height :200px"></div>
              <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDr5qwgemJp4LtodR8lvXg382V-cDFK3bY&callback=MapPosition"></script>

              <div class=" modal-footer">
                <input
                  type="button"
                  value="Đóng"
                  id="btnCancel"
                  class="btn btn-danger px-3"
                  data-dismiss="modal"
                />
              </div>
            </div>
          </div>
        </div>

        <!-- Modal Chi tiet TK -->
        <div id="ChiTietTK" class="modal fade" role="dialog">
          <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
              <div class="modal-header">
                <h4 class="modal-title text-uppercase">Chi tiết tài khoản</h4>
                <button type="button" class="close" data-dismiss="modal">
                  &times;
                </button>
              </div>

              <div class="modal-body">
                <table class="table table-sm table-borderless col-lg-8 mx-auto">
                  <tr>
                    <td>ID</td>
                    <td>B1606935</td>
                  </tr>
                  <tr>
                    <td>Họ tên</td>
                    <td>Bành Ngọc Thụy Thảo</td>
                  </tr>
                  <tr>
                    <td>Đơn vị</td>
                    <td>Khoa CNTT & TT</td>
                  </tr>
                  <tr>
                    <td>Loại tài khoản</td>
                    <td>Sinh viên</td>
                  </tr>
                  <tr>
                    <td>Hiệu lực</td>
                    <td>Còn hiệu lực</td>
                  </tr>
                </table>
              </div>

              <div class=" modal-footer">
                <input
                  type="button"
                  value="Đóng"
                  id="btnCancel"
                  class="btn btn-danger px-3"
                  data-dismiss="modal"
                />
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
